extends partials/layout

block title
    title=title

block content
    | Page #{pagination.num} of #{pagination.pages.length}
    br
    if pagination.next
        a(href=pagination.next.url) << Older
    if pagination.previous
        a(href=pagination.previous.url) >> Newer
    for post in pagination.files
        //| post
        a(href=post.url)
            h1=post.title
        p=post.snippet